{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys" : false,
    "import-sources-order":false,
    "named-imports-order": false,
    "grouped-imports":false,
    "ordered-imports": [
      false
    ]
  }
}
